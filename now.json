{
    "name": "eat-well-app2",
    "version": 2,
    "alias": "eat-well-app.now.sh",
    "builds": [
      { "src": "front-end/package.json", 
        "use": "@now/static-build",
        "config": { "distDir": "build" } 
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/Front-end/mealhelper/$1"
      }
    ],
    "github": {
      "enabled": true
    }
  }
  